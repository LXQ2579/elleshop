<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mars.elleshop.dao.InventoryDao">

    <select id="getInventory" resultType="int">
        select inventory_num from inventory where goods_type_id = #{goodsTypeId}
    </select>



