<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mars.elleshop.dao.CouponsDao">
    <select id="findCouponsById" resultType="coupons">
        select * from coupons
        where coupons_id = #{couponsId}
    </select>

    <select id="findAllCoupons" resultType="coupons">
        select * from coupons

    </select>
</mapper>